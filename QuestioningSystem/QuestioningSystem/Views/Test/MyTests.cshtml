@model  QuestioningSystem.Models.TestModels 
@{
    ViewBag.Title = "MyTests";
}

<h2 >MyTests</h2>


    <td> <input type="hidden" name="passId" value="1" /></td>
    <table class="table">

        <tr>
            <td class="labelStyle">Test Name</td>
            <td class="labelStyle">Date</td>
            <td class="labelStyle">Duration</td>
            <td class="labelStyle">Creator</td>
        </tr>

    @{

    foreach (var item in @Model.listOfTest)
    {
        <tr>
            <td> @item.TestName</td>
            <td> @item.DateTime</td>
            <td> @item.DurationTime min </td>
            <td> @item.Creator </td>
            <td><a href="JoinTest/@item.ID">Join test</a></td>
        </tr>
    }
    }
    </table>


