@model QuestioningSystem.Models.ListCompletedTestViewModel
@{
    ViewBag.Title = "CompletedTests";
}

<br />
<br />
<table class="table-bordered">
    <tr>
        <td class="labelStyle"> Test title </td>
        <td class="labelStyle"> Date time </td>
        <td class="labelStyle"> Maximum points </td>
        <td class="labelStyle"> Your points </td>
    </tr>
    @foreach (var item in @Model.list)
    {
        <tr>
            <td> @item.TestName</td> 
            <td> @item.DateTime </td>
            <td> @item.MaxPoints </td>
            <td> @item.NumberOfPoints </td>
        </tr>
    }
</table>

